fullpath = 'C:/Users/isaac/Projects/eyedata/etc_s020.EDF';

% [hdr, record] = edfread(fullpath);

edf = Edf2Mat(fullpath);

pupildiam = sqrt(edf.Samples.pa*4/pi);
eyeData = struct(...
    'gaze', struct(...
        'x', edf.Samples.(:)',...
        'y', edf.Samples.gy(:)'),...
    'diam', struct(...
        'left', pupildiam,...
        'right', pupildiam));